# Configurações de Ambiente - LSTM Stock Prediction
# Copie este arquivo para .env e ajuste os valores conforme necessário

# Diretórios de Artefatos
ARTIFACTS_DIR=./artifacts
MODEL_WEIGHTS_PATH=./artifacts/model_weights.pt
CONFIG_PATH=./artifacts/config.json
SCALER_PATH=./artifacts/scaler.pkl

# Configurações da API
API_HOST=0.0.0.0
API_PORT=8000
API_RELOAD=false

# Configurações de Treinamento
TRAINING_SYMBOL=NVDA
TRAINING_START_DATE=2020-01-01
TRAINING_END_DATE=2025-12-01
TRAINING_EPOCHS=100
TRAINING_BATCH_SIZE=32
TRAINING_LEARNING_RATE=0.001

# Redis (opcional)
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_DB=0

# Logging
LOG_LEVEL=INFO

# Python
PYTHONUNBUFFERED=1

